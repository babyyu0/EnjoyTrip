<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.nearby.area.SidoDao">
    <select id="select" parameterType="int" resultType="SidoVo">
        SELECT `code`, `name`
        FROM `sido`
        WHERE code = #{code};
    </select>
    <select id="selectAll" resultType="SidoVo">
        SELECT `code`, `name`
        FROM `sido`;
    </select>
</mapper>